   "source": [
    "set.seed(824)\n",
    "all_avgs <- c()\n",
    "for (i in 1:655) {\n",
    "    sample <- rbinom(54, 33, 0.77)\n",
    "    avg <- mean(sample)\n",
    "    all_avgs <- c(all_avgs, avg)\n",
    "}\n",
    "all_avgs_avg <- mean(all_avgs)\n",
    "all_avgs_avg\n",
    "vesp = 33 * 0.77\n",
    "vesp\n",
    "abs(all_avgs_avg - vesp)"
   ]
  }
